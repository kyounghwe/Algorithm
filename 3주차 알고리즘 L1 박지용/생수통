bottle_price = []
for i in range(3):
    bottle_price.append(int(input()))

cover_price = []
for i in range(2):
    cover_price.append(int(input()))
    
total_min_price = 0
bottle_min = 1000
cover_min = 1000

for j in bottle_price:
        if j<bottle_min:
            bottle_min = j
    
for j in cover_price:
    if j<cover_min:
        cover_min = j
            
total_min_price = bottle_min + cover_min + 10
print(total_min_price)

